{
  "name": "lokalise/php-lokalise-api",
  "type": "library",
  "description": "Lokalise API client",
  "homepage": "https://app.lokalise.com/api2docs/curl/",
  "keywords": ["REST", "API", "Lokalise"],
  "license": "MIT",
  "authors": [
    {
      "name": "Lokalise team",
      "homepage": "https://lokalise.co"
    }
  ],
  "require": {
    "ext-fileinfo": "*",
    "ext-json": "*",
    "php": ">=7.4",
    "php-http/guzzle7-adapter": "^0.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "php-http/mock-client": "^1.0",
    "guzzlehttp/psr7": "^1.2"
  },
  "autoload": {
    "psr-4": { "Lokalise\\": "Api/" }
  },
  "autoload-dev": {
    "psr-4": { "Lokalise\\Tests\\": "Tests/"}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.x.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
